<resources>

    <style name="AppBaseTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.



        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="bt_default" parent="@android:style/Widget.Button">
                <item name="android:textStyle">normal</item>
        <item name="android:textSize">@dimen/bt_text_size</item>
                <item name="android:textColor">@color/white</item>
                <item name="android:background">@drawable/bt_bg_default</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:paddingTop">0dp</item>
           </style>

    <style name="bt_with_icon" parent="@android:style/Widget.Button">
                <item name="android:textStyle">normal</item>
                <item name="android:textColor">@color/white</item>
                <item name="android:background">@drawable/btn_with_icon</item>
           </style>

    <style name="et_default" parent="@android:style/Widget.EditText">
                <item name="android:textStyle">normal</item>
                <item name="android:textColor">@color/white</item>
                <item name="android:background">@drawable/btn_with_icon</item>
           </style>

    <style name="memberinfo_ll">
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:background">@drawable/mytheme_textfield_default_holo_light</item>
    </style>

    <style name="memberinfo_tv">
        <item name="android:layout_gravity">bottom|center_horizontal</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="rb_textAppearance">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="progressdialog" parent="android:Theme.Holo.Dialog">
        <!--        <item name="android:alertDialogStyle">@style/CustomAlertDialogStyle</item> -->
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="CustomAlertDialogStyle">
        <item name="android:bottomBright">@color/transparent</item>
        <item name="android:bottomDark">@color/transparent</item>
        <item name="android:bottomMedium">@color/transparent</item>
        <item name="android:centerBright">@color/transparent</item>
        <item name="android:centerDark">@color/transparent</item>
        <item name="android:centerMedium">@color/transparent</item>
        <item name="android:fullBright">@color/transparent</item>
        <item name="android:fullDark">@color/transparent</item>
        <item name="android:topBright">@color/transparent</item>
        <item name="android:topDark">@color/transparent</item>
    </style>

</resources>
